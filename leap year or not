function is LeapYear(year):
    if year modulo 4 equals 0:
        if year modulo 100 is not equal to 0 or year modulo 400 equals 0:
            return true
    return false

// Example 
year = 2024
if isLeapYear(year):
    print(year + " is a leap year")
else:
    print(year + " is not a leap year")
